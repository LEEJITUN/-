<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.team.project.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_talkFragment"
            app:destination="@id/talkFragment" />
        <action
            android:id="@+id/action_homeFragment_to_tipFragment"
            app:destination="@id/tipFragment" />
        <action
            android:id="@+id/action_homeFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
        <action
            android:id="@+id/action_homeFragment_to_webViewFragment"
            app:destination="@id/webViewFragment" />
        <action
            android:id="@+id/action_homeFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/tipFragment"
        android:name="com.team.project.fragments.TipFragment"
        android:label="fragment_tip"
        tools:layout="@layout/fragment_tip" >
        <action
            android:id="@+id/action_tipFragment_to_talkFragment"
            app:destination="@id/talkFragment" />
        <action
            android:id="@+id/action_tipFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_tipFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
        <action
            android:id="@+id/action_tipFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_tipFragment_to_webViewFragment"
            app:destination="@id/webViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/talkFragment"
        android:name="com.team.project.fragments.TalkFragment"
        android:label="fragment_talk"
        tools:layout="@layout/fragment_talk" >
        <action
            android:id="@+id/action_talkFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_talkFragment_to_tipFragment"
            app:destination="@id/tipFragment" />
        <action
            android:id="@+id/action_talkFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
        <action
            android:id="@+id/action_talkFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_talkFragment_to_webViewFragment"
            app:destination="@id/webViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/myInfoFragment"
        android:name="com.team.project.fragments.MyInfoFragment"
        android:label="MyInfoFragment" >
        <action
            android:id="@+id/action_myInfoFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_myInfoFragment_to_myInfoChangePwFragment"
            app:destination="@id/myInfoChangePwFragment" />
        <action
            android:id="@+id/action_myInfoFragment_to_myInfoDeleteFragment"
            app:destination="@id/myInfoDeleteFragment" />
        <action
            android:id="@+id/action_myInfoFragment_to_loginActivity"
            app:destination="@id/loginActivity" />
        <action
            android:id="@+id/action_myInfoFragment_to_talkFragment"
            app:destination="@id/talkFragment" />
        <action
            android:id="@+id/action_myInfoFragment_to_tipFragment"
            app:destination="@id/tipFragment" />
        <action
            android:id="@+id/action_myInfoFragment_to_myInfoNfcFragment"
            app:destination="@id/myInfoNfcFragment" />
        <action
            android:id="@+id/action_myInfoFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>

    <fragment
        android:id="@+id/myInfoChangePwFragment"
        android:name="com.team.project.MyInfoChangePwFragment"
        android:label="MyInfoChangePwFragment" >
        <action
            android:id="@+id/action_myInfoChangePwFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/myInfoDeleteFragment"
        android:name="com.team.project.MyInfoDeleteFragment"
        android:label="MyInfoDeleteFragment" >
        <action
            android:id="@+id/action_myInfoDeleteFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
    </fragment>
    <activity
        android:id="@+id/loginActivity"
        android:name="com.team.project.auth.LoginActivity"
        android:label="LoginActivity" />
    <fragment
        android:id="@+id/webViewFragment"
        android:name="com.team.project.fragments.WebViewFragment"
        android:label="webview"
        tools:layout="@layout/webview" >
        <action
            android:id="@+id/action_webViewFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_webViewFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
        <action
            android:id="@+id/action_webViewFragment_to_talkFragment"
            app:destination="@id/talkFragment" />
        <action
            android:id="@+id/action_webViewFragment_to_tipFragment"
            app:destination="@id/tipFragment" />
        <action
            android:id="@+id/action_webViewFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/myInfoNfcFragment"
        android:name="com.team.project.fragments.MyInfoNfcFragment"
        android:label="MyInfoNfcFragment" >
        <action
            android:id="@+id/action_myInfoNfcFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.team.project.KakaoMapAPI.MapFragment"
        android:label="MapFragment" >
        <action
            android:id="@+id/action_mapFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_mapFragment_to_webViewFragment"
            app:destination="@id/webViewFragment" />
        <action
            android:id="@+id/action_mapFragment_to_talkFragment"
            app:destination="@id/talkFragment" />
        <action
            android:id="@+id/action_mapFragment_to_myInfoFragment"
            app:destination="@id/myInfoFragment" />
        <action
            android:id="@+id/action_mapFragment_to_tipFragment"
            app:destination="@id/tipFragment" />
    </fragment>
</navigation>